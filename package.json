{
  "name": "DietDoctor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "android:buildDebug": "cd ./android && ./gradlew assembleDebug && cd ../",
    "android:buildRelease": "cd ./android && ./gradlew assembleRelease && cd ../",
    "android:installDebug": "adb install  ./android/app/build/outputs/apk/debug/app-debug.apk",
    "android:reinstallDebug": "adb install -r ./android/app/build/outputs/apk/debug/app-debug.apk",
    "android:installRelease": "adb install  ./android/app/build/outputs/apk/release/app-release.apk",
    "android:reinstallRelease": "adb install -r ./android/app/build/outputs/apk/release/app-release.apk",
    "clean:cache": "yarn cache clean",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean:start": "yarn start --reset-cache",
    "postinstall": "npx jetify",
    "adb": "adb reverse tcp:8081 tcp:8081"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/push-notification-ios": "^1.8.0",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "axios": "^0.21.1",
    "lodash": "^4.17.20",
    "memoize-one": "^5.1.1",
    "moment": "^2.29.1",
    "moment-jalaali": "^0.9.2",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "1.9.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^2.0.1",
    "react-native-modal": "^11.6.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.16.1",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-share": "^5.1.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swipe-list-view": "^3.2.5",
    "react-native-tab-view": "^2.15.2",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.16"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@svgr/core": "^5.4.0",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.155",
    "@types/moment-jalaali": "^0.7.4",
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.62.18",
    "@types/react-native-push-notification": "^3.0.9",
    "@types/react-native-share": "^3.1.0",
    "@types/react-native-snap-carousel": "^3.8.1",
